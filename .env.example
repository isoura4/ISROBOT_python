# Configuration du bot Discord
# Remplacez ces valeurs par vos vraies valeurs

# ID de votre application Discord
app_id=123456789012345678

# Token secret de votre bot
secret_key=VOTRE_TOKEN_ICI

# ID de votre serveur Discord
server_id=123456789012345678

# Configuration de la base de données
# Chemin vers votre base de données SQLite
db_path=./database.sqlite3

# Configuration Twitch API
# Obtenez ces valeurs sur https://dev.twitch.tv/console
twitch_client_id=VOTRE_CLIENT_ID_TWITCH
twitch_client_secret=VOTRE_CLIENT_SECRET_TWITCH